@charset "utf-8";
/* CSS Document */
body,ul,ol,li,p,h1,h2,h3,h4,h5,h6,form,fieldset,table,td,img,div,dl,dt,dd{margin:0;padding:0;border:0;}
body{font:12px/1.5 "Microsoft Yahei", arial, "Hiragino Sans GB", sans-serif;}

ul,ol{list-style-type:none;}
input,img,select{vertical-align:middle;}
a{text-decoration:none;color:#000;}
label{vertical-align:middle}
table {border-collapse:collapse;border-spacing:0;}
dt{float:left;height:24px;line-height:25px;text-align:right; padding-top:14px;width:200px;font-weight:bold;margin-right:5px;}
dd{padding-top:14px;text-align:left;height:24px;line-height:25px;}
.clearfix:after{content:".";display:block;clear:both;visibility:hidden;line-height:0;height:0;}
.clearfix{zoom:1;}

.agreement h4{
	font-size: 14px;
	line-height: 32px;margin:5px 0;
}
div .agreement p{
	margin:0px;
	padding:0px;
}