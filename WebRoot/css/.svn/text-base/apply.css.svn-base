
.applyC{width:960px;padding:10px 0;}
.applyCL{width:419px;float:left;text-align:center;}
.p-bottom{padding-bottom:150px;}
.applyCR{float:left;width:540px;position:relative;border-left:1px solid #d0d0d0;text-align:left;}
.pointBlue{
	display:block;
	width:15px;height:30px;
	position:absolute;
	left:0;
	background:url(../images/r-angle.png) no-repeat;}

.applyCLT{width:330px;border-bottom:1px solid #2da7e0;margin:20px auto;}
.applyCLT a,.applyCLT a:visited{display:inline-block;width:104px;height:35px;color:#414141;font-family:\5FAE\8F6F\96C5\9ED1,'Microsoft YaHei',STHeiti,sans-serif;text-align:center;text-decoration:none;background-image:url(../images/a-bg.png);margin-left:36px;line-height:35px;
	-webkit-border-radius: 5px 5px 0px 0px;
	-moz-border-radius: 5px 5px 0px 0px;
	-o-border-radius: 5px 5px 0px 0px;
	-ms-border-radius: 5px 5px 0px 0px;
	border-radius: 5px 5px 0px 0px;
}
.applyCLT a:hover,.applyCLT a.curr,.applyCLT a:visited.curr{background-image:url(../images/a-click.png);color:#fff;}
.applyFo{padding-left: 30px;}
.applyC input.foBtn{background-color:#2da7e0;}
.applyC input:hover.foBtn{background-color:#1c75bc;}

.applyCR-con{padding-top: 10px;}
.applyCR-con h4{color:#2da7e0;font-size:16px;font-family:\5FAE\8F6F\96C5\9ED1,'Microsoft YaHei',STHeiti,sans-serif;padding:8px;margin-left: 20px;font-weight:normal;}
.applyCR-con ul{margin-left: 29px;color:#696969;margin-bottom: 10px;}
.applyCR-con img{margin-left: 20px;margin-top: 10px;}

.borrowing_calculator {
border: 1px solid #e4e4e4;
background: #fcfcfc;
padding: 15px;
margin:20px 10px;
}
.borrowing_calculator h3 {
font-size: 16px;
font-weight:normal;
}
.loan_box h3, .borrowing_calculator h3 {
padding-bottom: 10px;
}
.calculators_box {
width: 160px;
}
/*==== form ====*/
.m-row,.m-page{*zoom:1;}.m-row:after,.m-page:after{content:"\200B"; display:block; height:0; clear:both;overflow:hidden; visibility:hidden; }
.m-row .m-rowLabel{width:70px;text-align:right;color:#414141;margin-right: 5px;height:34px;line-height:34px;}
.m-row .m-drop,.m-row .m-textFocus{height:32px; line-height:32px;}
.m-row-mini .m-rowLabel,.m-row-mini .m-textFocus,.m-row-mini .m-drop{height:24px; line-height:24px;}
.m-row-mini .m-rowLabel{line-height:24px;}
.m-rowTextarea{border:1px solid #ccc;background-color:#fff;}
.m-row .m-holderBox input{line-height:34px;}
.m-row.m-row-mini .m-holderBox input{line-height:24px;}
.m-textFocus {
border: 1px solid #d7d8d8;
text-indent: 12px;
display: block;
}
.m-holderInput {
background-color: none;
width: 100%;
text-indent: 12px;
}
.icon-yuan {
background:url(../images/icon-yuan.jpg) no-repeat right 50%;
}
.icon-per{
	background:url(../images/icon-per.jpg) no-repeat right 50%;}
.m-dropInput {
border: 0;
text-indent: 12px;
color: #aaa;
cursor: text;
overflow: hidden;
}
.m-dropInput {
float: left;
}
.m-drop {
border: 1px solid #d7d8d8;
}
.m-drop {
position: relative;
}

select,.period{
	width:83px;*width:80px;*margin-left:-5px;*margin-top: 0px;padding:5px;height:32px;line-height:32px;border:0;text-indent: 5px;}
.now_calculate {
text-align: right;
}
.now_calculate .btn_normal {
width: 142px;
height: 35px;
line-height: 35px;
}
.btn_normal {
width: 120px;
}
.input_btn {
height: 30px;
line-height: 30px;
vertical-align: middle;
padding: 0 20px;
display: inline-block;
border: 1px solid #e7e8e8;
text-align: center;
font-family: "microsoft yahei";
font-size: 16px;
cursor: pointer;
margin: 0;
overflow: visible;
width: auto;
text-transform: uppercase;
color: #2da7e0;
background: #fafbfc;
border: 1px solid #2da7e0;
vertical-align: middle;
overflow-y: hidden;
blr: expression(this.onFocus=this.blur());
}
.calculators_result {
width:303px;
padding-left: 25px;
}
.calculators_result a.detail,.calculators_result a.detail:visited{color:#2da7e0;}
table {
display: table;
border-collapse: separate;
border-spacing: 2px;
border-color: gray;
}
tr {
display: table-row;
vertical-align: inherit;
border-color: inherit;
}
.calculators_result table th {
text-align: left;
font-weight:normal;
border-bottom: 1px solid #f0f0f0;
padding: 0 0 10px 0;
}
.calculators_result table td {
padding: 4px 0;
}
.calculators_result table td strong {
padding: 0 5px 0 10px;
}
.money {
color: #c10000;
}
element.style {
}
.calculators_result table td {
padding: 4px 0;
}
.calculators_result td,.calculators th {display: table-cell;vertical-align: inherit;font:12px/1.5 tahoma,arial,\5b8b\4f53,SimSun,STSong,sans-serif;color: #414141;}


.ui-breadcrumb{width: 1000px;height: 50px;margin: 0 auto;padding: 10px 0; }
.ui-breadcrumb ul li{float: left;;color: #555;line-height: 50px}
.ui-breadcrumb ul li a{color: #555;}
.ui-breadcrumb-text-first{margin-top: 0;font-size: 18px;}
.ui-breadcrumb-text{font-size: 14px;}
.ui-breadcrumb-separator {
margin:0 8px;
width: 9px;
height: 14px;font-size: 18px;}
