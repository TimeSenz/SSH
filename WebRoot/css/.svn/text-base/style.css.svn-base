.shoudaitong{text-align: center;background-color:#2da7e0;box-shadow: 0 2px 2px rgba(0,0,0,0.2);
	position: relative; color:#FFF}
.shoudaitong .sdt-con{padding-left:0;padding-right:0;overflow:hidden;}
.shoudaitong h3{
	font-size: 18px;
	margin-bottom: 10px;
